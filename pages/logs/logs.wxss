.shopbag{
    background: #6f5b5b;
    background-size: 750rpx auto;
    height: 260rpx; 
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.shopimg{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
}
.shopimg image{
    width: 100%;
    height: 100%;
}
.shopinfo{
    width: 60%;
    color: #fff;
}
.food{
  display: flex;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
.foodimg{
  width: 120rpx;
  height: 120rpx;
}
.foodimg image{
  width: 100%;
  height: 100%;
}
.foodinfo{
  flex: 1;
  padding-left: 10rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 24rpx;
}
.playcontent{
  position: fixed;
  top: 260rpx;
  bottom: 100rpx;
}
.menuleft{
  background: #ccc;
  height: 100%;
  width: 200rpx;
  float: left;
}
.nav{
  line-height: 100rpx;
  text-align: center;
}
.nav.on{
  background: #fff;
  border-left: 2px solid #f00;
}
.menuright{
  width: 550rpx;
  height: 100%;
  float: right;
}
.footerbar{
  background: rgba(0, 0, 0, 0.6);
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 0px;
  color: #fff;
}
.btnimg{
  width: 100rpx;
  height: 100rpx;
  background: #303030;
  border-radius: 50%;
  padding: 10rpx;
  position: absolute;
  left: 30rpx;
  top: -40rpx;
}
.btnimg image{
  width: 100%;
  height: 100%;
}